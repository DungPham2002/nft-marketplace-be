{
    "version": 2,
	"builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
    "rewrites": [{ "source": "/api/v1/(.*)", "destination": "/api/v1" }]
}